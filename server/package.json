{
  "name": "fitness-tracker-sync-server",
  "version": "1.0.0",
  "description": "Node API for syncing Fitness Tracker data to Supabase",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "watch": "tsc -w",
    "dev:watch": "concurrently \"npm run build -- --watch\" \"nodemon dist/index.js\"",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push:pg",
    "db:pull": "drizzle-kit pull:pg",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "fitness",
    "tracker",
    "sync",
    "supabase"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@garmin/fitsdk": "^21.171.0",
    "@supabase/supabase-js": "^2.39.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.31.7",
    "drizzle-orm": "^0.44.7",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3",
    "postgres": "^3.4.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.7",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
